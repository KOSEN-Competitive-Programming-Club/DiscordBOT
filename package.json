{
  "name": "DiscordBOT",
  "packageManager": "yarn@4.0.0-rc.48",
  "volta": {
    "node": "18.17.1",
    "yarn": "4.0.0-rc.48"
  },
  "dependencies": {
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "start": "yarn tsc && yarn node build/botmain.js",
    "build": "yarn tsc",
    "dev": "yarn ts-node src/botmain.ts",
    "Linter": "yarn eslint --fix && yarn prettier src --write ",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@types/eslint": "^8",
    "@types/prettier": "^3",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn Linter"
    ]
  }
}
